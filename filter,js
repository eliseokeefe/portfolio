  function filterItems() {
    const field = document.getElementById("field-filter").value;
    const lang = document.getElementById("lang-filter").value;
    let result = "";
  
    if (data[field]) result += data[field];
    if (data[lang] && lang !== field) result += data[lang];
  
    document.getElementById("output").innerHTML = result || "<p>Please select a category to view results.</p>";
  }
  
